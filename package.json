{
  "name": "nodejs-starter-project",
  "version": "1.0.0",
  "description": "Starter template for nodejs rest api project",
  "main": "app.js",
  "scripts": {
    "start:dev": "nodemon --exec \"npm run build && node build/bundle.js\" -e js --ignore build/",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "rimraf build && webpack --mode=development",
    "build:prod": "rimraf build && webpack",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "keywords": [
    "nodejs",
    "restapi"
  ],
  "author": "Subhadip Majumder",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.2",
    "eslint": "8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.4.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@sentry/node": "^7.44.2",
    "@sentry/tracing": "^7.44.2",
    "@types/jest": "^29.4.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-validator": "^6.15.0",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.0",
    "morgan": "^1.10.0",
    "razorpay": "^2.8.6",
    "stripe": "^12.9.0",
    "uuid": "^9.0.0",
    "validator": "^13.9.0",
    "winston": "^3.8.2"
  }
}
